<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="epolicy.ajsmsig.co.id.epolicy">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="epolicy.ajsmsig.co.id.epolicy.Activity.SplashActivity"
            android:theme="@style/Theme.AppCompat.Light.NoActionBar.FullScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="epolicy.ajsmsig.co.id.epolicy.Activity.MainActivity" />
        <!--<activity android:name="epolicy.ajsmsig.co.id.epolicy.Activity.RegisterActivity" />-->
        <activity android:name="epolicy.ajsmsig.co.id.epolicy.Activity.LoginActivity"></activity>
    </application>



</manifest>
